{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "ml/sparkling-h20",
      "network": "HOST",
      "forcePullImage": true,
      "portMappings": [ ]
    },
	"volumes": [
	]
  },
  "id": "/ml/sparkling-h20-mesos-docker",
  "instances": 1,
  "cpus": 1,
  "mem": 1024,
  "uris": [],
  "env":{
	  "MASTER": "your mesos master",
	  "SPARK_MESOS_EXECUTOR_HOME":"/usr/local/spark-1.5.1-bin-hadoop2.6",
	  "SPARK_ADDITIONAL_CONFIG":"--conf spark.executor.memory=2g --conf spark.mesos.executor.docker.image=ml/sparkling-h20",
	  "SPARK_CORES_MAX":"8",
	  "SPARK_JOB":"--packages ai.h2o:sparkling-water-core_2.10:1.5.1 --class org.apache.spark.examples.h2o.AirlinesWithWeatherDemo /dev/null",
	  "SPARK_ADDITIONAL_JARS":"--jars /sparkling-water/sparkling-water-examples_2.10-0.2.17-SNAPSHOT.jar",
	  "SPARK_USE_DOCKER_HOST":"true"
  }
}
